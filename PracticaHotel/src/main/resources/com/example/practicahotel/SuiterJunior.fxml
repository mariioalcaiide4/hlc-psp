<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="com.example.practicahotel.controller.SuiteIndividualController">
    <ImageView fx:id="imageView" fitHeight="285.0" fitWidth="500" preserveRatio="true" />
    <Slider fx:id="slider" blockIncrement="1" max="4" min="0" snapToTicks="true" />
    <ProgressIndicator fx:id="progressIndicator" layoutX="50" layoutY="50" prefWidth="100" prefHeight="100" />
</VBox>
